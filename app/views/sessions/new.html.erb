<main>
  <h1>Log in to access your account</h1>
  <%= form_for :session, url: login_url do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :remember_me %>
    <%= f.check_box :remember_me %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit 'Submit' %>
  <% end %>
</main>